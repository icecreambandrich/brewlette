{
  "name": "brewlette",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && prisma db push && next build",
    "start": "next start",
    "lint": "eslint",
    "setup": "npx prisma generate && npx prisma db push && npx tsx scripts/setup.ts",
    "db:reset": "npx prisma db push --force-reset && npx tsx scripts/setup.ts",
    "backfill:postcodes": "npx tsx scripts/backfill-postcodes.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "csv-parse": "^6.1.0",
    "next": "15.5.3",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
